# Config for z3s

CONFIG_MODEL_Z3S=y

# Camera
# CONFIG_SENSOR_RETENTION_USE is not set
# CONFIG_CAMERA_HST_V01 is not set
CONFIG_CAMERA_HST_V03=y
# CONFIG_CAMERA_CIS_3J1_OBJ is not set
# CONFIG_CAMERA_CIS_GW2_OBJ is not set
# CONFIG_CAMERA_CIS_2LA_OBJ is not set
# CONFIG_CAMERA_CIS_2LD_OBJ is not set
CONFIG_CAMERA_CIS_GH1_OBJ=y
CONFIG_CAMERA_CIS_2L3_OBJ=y
CONFIG_CAMERA_CIS_HM1_OBJ=y
CONFIG_CAMERA_CIS_TOF_BUCK_USE=y
CONFIG_CAMERA_CIS_IMX516_OBJ=y
CONFIG_CAMERA_CIS_IMX518_OBJ=y
CONFIG_CAMERA_CIS_IMX586_OBJ=y

# Wireless Charging
CONFIG_WIRELESS_CHARGER_MFC_S2MIW04=y
CONFIG_WIRELESS_CHARGER_HAL_MFC=y
CONFIG_SENSORS_A96T3X6=y
CONFIG_SENSORS_FW_VENDOR=y
CONFIG_SENSORS_A96T365IF=y
CONFIG_SENSORS_A96T3X6_CRC_CHECK=y
CONFIG_SENSORS_A96T3X6_BLOCK_SAR_ONLY=y

# Samsung Galaxy S20 Ultra (z3s) comprehensive kernel config for Droidian/Halium support

# Basic device architecture
CONFIG_ARM64=y
CONFIG_CPU_BIG_LITTLE=y
CONFIG_EXYNOS_GIC_V3=y
CONFIG_SOC_EXYNOS990=y

# Core Android and Halium support
CONFIG_ANDROID=y
CONFIG_ANDROID_BINDER_IPC=m
CONFIG_ANDROID_BINDER_IPC_SELFTEST=y
CONFIG_ANDROID_PARANOID_NETWORK=n
CONFIG_ANDROID_RAM_CONSOLE=y
CONFIG_ANDROID_VIRT_DRIVERS=y
CONFIG_ANDROID_BOOT_PARAM=y
CONFIG_ANDROID_LOGGER=y
CONFIG_ANDROID_LOW_MEMORY_KILLER=y

# Filesystem support
CONFIG_F2FS_FS=y
CONFIG_F2FS_FS_XATTR=y
CONFIG_UBIFS_FS=y
CONFIG_EXT4_FS=y
CONFIG_EXFAT_FS=y
CONFIG_ZRAM=y

# Security
CONFIG_SECURITY=y
CONFIG_SECURITY_SELINUX=y
CONFIG_SECURITY_APPARMOR=m
CONFIG_SECURITY_LOCKDOWN_LSM=y
CONFIG_KEYS=y
CONFIG_KEYS_COMPAT=y
CONFIG_CRYPTO_USER_API_HASH=y

# Containers and namespaces for LXC
CONFIG_NAMESPACES=y
CONFIG_CGROUPS=y
CONFIG_CGROUP_CPUACCT=y
CONFIG_CGROUP_DEVICE=y
CONFIG_CGROUP_SCHED=y
CONFIG_CGROUP_FREEZER=y
CONFIG_CGROUP_BPF=y
CONFIG_PID_NS=y
CONFIG_IPC_NS=y
CONFIG_NET_NS=y
CONFIG_UTS_NS=y
CONFIG_DEVPTS_MULTIPLE_INSTANCES=y

# Devtmpfs for device nodes
CONFIG_DEVTMPFS=y
CONFIG_DEVTMPFS_MOUNT=y

# Samsung and Exynos specific
CONFIG_EXYNOS_VDD_CPU=y
CONFIG_EXYNOS_THERMAL=y
CONFIG_EXYNOS_PM_QOS=y
CONFIG_SAMSUNG_VBMETA_IS_SUPPORTED=y
CONFIG_SEC_ABC=y
CONFIG_SEC_QC_FEATURE=y

# Networking
CONFIG_NETFILTER=y
CONFIG_NETFILTER_XT_MATCH_STATE=y
CONFIG_NETFILTER_XT_TARGET_MASQUERADE=y
CONFIG_IP_MULTICAST=y
CONFIG_IPV6=y

# Power management
CONFIG_CPU_FREQ_GOV_ONDEMAND=y
CONFIG_CPU_FREQ_GOV_SCHEDUTIL=y
CONFIG_CPU_IDLE=y

# Kernel modules and debug
CONFIG_MODULES=y
CONFIG_DYNAMIC_DEBUG=y
CONFIG_DEBUG_INFO=y
CONFIG_DEBUG_KERNEL=y
CONFIG_FTRACE=y
CONFIG_KPROBES=y
CONFIG_TRACEPOINTS=y
CONFIG_DEBUG_FS=y
CONFIG_PROC_FS=y
CONFIG_KALLSYMS=y
CONFIG_CONSOLE_POLL=y
CONFIG_DEBUG_BUGVERBOSE=y

# Kernel command line defaults
CONFIG_CMDLINE="console=tty0 androidboot.selinux=permissive lxc.enable=1 droidian.lvm.prefer=1"

# Additional hardware support
CONFIG_MEDIA_SUPPORT=y
CONFIG_USB_SUPPORT=y
